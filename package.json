{
  "name": "stackunity",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20.11.1",
    "npm": "10.2.4"
  },
  "scripts": {
    "build": "NODE_OPTIONS='--max-old-space-size=8192 --max-semi-space-size=512' nuxt build --no-generate",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start": "node .output/server/index.mjs",
    "upload-chromium": "node scripts/upload-chromium.js",
    "git": "git add . && git commit -m 'build' && git push origin main",
    "git-remove": "git rm -r --cached server/utils server/api/payment server/api/auth server/api/user server/api/analyze stores/ do.md pages/website-analyzer.vue components/seo && git commit -m \"Remove sensitive files\" && git push origin main"
  },
  "config": {
    "rollup": {
      "platform": "linux",
      "arch": "x64"
    }
  },
