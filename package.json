{
  "name": "stackunity",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20.11.1",
    "npm": "10.2.4"
  },
  "scripts": {
    "build": "NODE_OPTIONS='--max-old-space-size=6144' nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start": "node .output/server/index.mjs",
    "upload-chromium": "node scripts/upload-chromium.js",
    "save-config": "node scripts/preserve-config.js save",
    "restore-config": "node scripts/preserve-config.js restore",
    "git": "npm run save-config && git add . && git commit -m 'build' && git push origin main && npm run restore-config",
    "git-clean": "node scripts/git-cleaner.js"
  },
  "config": {
    "rollup": {
      "platform": "linux",
      "arch": "x64"
    }
  },
  "dependencies": {
    "@codemirror/lang-sql": "^6.8.0",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@mdi/font": "^7.4.47",
    "@nuxtjs/i18n": "^9.5.4",
    "@pinia/nuxt": "^0.10.1",
    "@stripe/stripe-js": "^6.1.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cheerio": "^0.22.35",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/pdfkit": "^0.13.9",
    "axios": "^1.8.1",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.4.9",
    "cheerio": "^1.0.0",
    "echarts": "^5.6.0",
    "highlight.js": "^11.11.1",
    "jsonwebtoken": "^9.0.2",
    "mssql": "^11.0.1",
    "mysql2": "^3.9.2",
    "node-sql-parser": "^5.3.9",
    "nuxt": "^3.15.4",
    "pdfkit": "^0.16.0",
    "pg": "^8.16.0",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "resend": "^4.1.2",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "stripe": "^18.0.0",
    "uuid": "^9.0.0",
    "vite-plugin-vuetify": "^2.1.0",
    "vue": "latest",
    "vue-chartjs": "^5.3.2",
    "vue-codemirror": "^6.1.1",
    "vue-echarts": "^7.0.3",
    "vue-router": "latest",
    "vuetify": "^3.7.13"
  },
  "devDependencies": {
    "@types/node": "^22.13.5",
    "@types/uuid": "^10.0.0"
  }
}
